<view class="table">
  <view class="tr bg-w">
    <view class="th">预约时间</view>
    <view class="th">课程名称</view>
  </view>
  <block wx:for="{{listData}}" wx:key="{{code}}">
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
      <view class="td">{{item.date}}</view>
      <view class="td">{{item.title}}</view>
    </view>
    <view class="tr" wx:else>
      <view class="td">{{item.date}}</view>
      <view class="td">{{item.title}}</view>
    </view>
  </block>
</view>
